	{{#if resume.education.length}}
	<section id="education">
		<h2>Education</h2>
		{{#each resume.education}}
		<div class="item">
			{{#if studyType}}
				<h3 class="studyType">
					{{studyType}} in
					{{#if area}}
						{{area}}
					{{/if}}
				</h3>
			{{/if}}
			<div class="details">
				{{#if institution}}
					<span class="name">
						{{institution}}
					</span>
				{{/if}}
				{{#if startDate}}
				<span class="startDate">
					{{startDate}}
				</span>
				{{/if}}
				{{#if endDate}}
				<span class="endDate">
					- {{endDate}}
				</span>
				{{else}}
				<span class="endDate">
					- Present
				</span>
				{{/if}}

				{{#if gpa}}
					<div class="gpa">
						GPA: {{gpa}}
					</div>
				{{/if}}
				{{#if courses.length}}
					<ul class="courses">
						{{#each courses}}
							<li>{{.}}</li>
						{{/each}}
					</ul>
				{{/if}}
			</div>
		</div>
		{{/each}}
	</section>
	{{/if}}

	{{#if resume.portfolio.length}}
	<section id="additional-urls">
		<h2>Additional URL's</h2>
